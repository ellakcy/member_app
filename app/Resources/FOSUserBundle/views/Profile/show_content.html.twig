{% trans_default_domain 'FOSUserBundle' %}

    <div class="box box-widget widget-user" itemscope itemtype="http://schema.org/Person">
    	<div class="widget-user-header">
    	    <h3>{{ user.username }}</h3>
    	</div>
    	<div class="widget-user-image">
    			<img class="img-circle" src="data:image/svg+xml;utf8;base64," />
    	</div>
    	<div class="box-footer">
            <p>{{ 'profile.show.email'|trans }}: <span itemprop="email">{{ user.email }}</span></p>
    	</div>
    </div>
