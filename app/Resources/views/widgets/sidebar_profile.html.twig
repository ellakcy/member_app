{% trans_default_domain 'panel' %}

<div class="row text-center" itemscope itemtype="http://schema.org/Person">
  <div class="col-12">
    <img class="rounded-circle mt-2 bg-light" src="data:image/svg+xml;utf8;base64," /><br>
  </div>
  <div class="col-12">
    <h4 class="sidebar_header" > <span itemprop="name">{{ user.name }}</span>&nbsp;<span itemprop="surname">{{ user.surname }}</span></h4>
  </div>
  <div class="col-12 sidebar_font">
    {{user.username}}@ellak.org.cy
  </div>
  <div class="col-12">
    <a href="{{ path('fos_user_profile_edit') }}">{{ 'panel.edit.profile'|trans }}</a>&nbsp;-&nbsp;<a href="{{ path('fos_user_security_logout') }}" class="text-danger">{{ 'panel.logout'|trans }}</a>
  </div>
</div>
